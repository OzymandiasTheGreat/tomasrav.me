{
  "name": "ozymandias.tk",
  "version": "0.2.0",
  "description": "Personal website for Tomas Ravinskas",
  "main": "src/assets/main.js",
  "scripts": {
    "wait": "delay 3",
    "clean": "rimraf dist/*",
    "build:hangman": "ncp hangman/dist/ dist/hangman/",
    "build:dist": "NODE_ENV=production rollup -c",
    "build": "npm-run-all -s clean build:*",
    "serve": "NODE_ENV=development rollup -c --watch",
    "open": "firefox -private-window localhost:3000/index",
    "test:prepare": "npm-run-all -s serve",
    "test:interact": "npm-run-all -s wait open",
    "test": "npm-run-all -p test:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OzymandiasTheGreat/ozymandias.tk.git"
  },
  "keywords": [
    "personal",
    "website'"
  ],
  "author": "Tomas Ravinskas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OzymandiasTheGreat/ozymandias.tk/issues"
  },
  "homepage": "https://github.com/OzymandiasTheGreat/ozymandias.tk#readme",
  "browserslist": "> 2%, not dead, not ie < 10, not OperaMini all",
  "dependencies": {
    "jquery": "^3.4.1",
    "js-cookie": "^2.2.0",
    "materialize-css": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "bs-console-qrcode": "file:../bs-console-qrcode",
    "cookie-parser": "^1.4.4",
